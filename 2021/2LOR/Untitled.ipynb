    "def babelek(t):\n",
    "    for i in range(len(t)-1):\n",
    "        if t[i] < t[i+1]:\n",
    "            t[i], t[i+1] = t[i+1], t[i]\n",
    "    return t\n",
    "\n",
    "t = [1,5,3,2,6]\n",
    "print(babelek(t))"
   ]
